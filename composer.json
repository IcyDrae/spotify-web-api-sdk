{
    "name": "rgjoni/spotify-web-api-sdk",
    "description": "Spotify Web API SDK for PHP",
    "authors": [
        {
            "name": "Reard Gjoni",
            "email": "gjoni-r@hotmail.com"
        }
    ],
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Gjoni\\SpotifyWebApiSdk\\": "src/",
            "Gjoni\\SpotifyWebApiSdk\\Tests\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "require": {
        "php": ">7.4",
        "ext-json": "*",
        "ext-http": "*",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "ext-xdebug": "*"
    },
    "minimum-stability": "stable"
}
